{
  "name": "react-hooks-uikit-express-oauth-boiler",
  "version": "2.0.0",
  "description": "A React+Redux boilerplate using Express as backend, UIKit for frontend, MongoDB for storage & open-authenticator for auth.",
  "main": "dist/server/index.js",
  "scripts": {
    "deploy": "echo \"Error: no deployment options yet\" && exit 1",
    "build": "webpack --optimization-minimize --optimization-node-env production --mode production && tsc",
    "dev": "concurrently \"yarn run dev:client\" \"yarn run dev:server\"",
    "dev:server": "nodemon --watch 'src/**' --ext 'ts,json' --exec 'ts-node src/server/index.ts'",
    "dev:client": "webpack serve --host 0.0.0.0",
    "start": "cross-env NODE_ENV=production node dist/server/src/server/index",
    "launch": "yarn run build; yarn run start",
    "setup": "node scripts/setup.js",
    "setup:default": "node scripts/setup.default.js",
    "config": "yarn run setup",
    "test": "jest --verbose",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "author": "Scharkee",
  "repository": {
    "type": "git",
    "url": "https://repo.hepha.co.uk/root/react-redux-passport-uikit-express-boiler"
  },
  "license": "ISC",
  "dependencies": {
    "await-to-js": "^2.1.1",
    "axios": "^0.21.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.0",
    "connect-mongo": "^3.0.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "faker": "^5.1.0",
    "fs-extra": "^9.0.1",
    "helmet": "^4.2.0",
    "mongoose": "^5.11.8",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "react": "^17.0.1",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dom": "^17.0.1",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "ts-node": "^9.1.1",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.1.3",
    "uikit": "^3.6.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.5.5",
    "@types/bcrypt": "^3.0.0",
    "@types/express-session": "^1.17.3",
    "@types/passport": "^1.0.4",
    "@types/react": "^17.0.0",
    "@types/react-router-dom": "^5.1.6",
    "babel-loader": "^8.0.6",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "nodemon": "^2.0.6",
    "prompt-sync": "^4.1.7",
    "style-loader": "^2.0.0",
    "svg-inline-loader": "^0.8.0",
    "ts-loader": "^8.0.12",
    "webpack": "^5.10.1",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/test/__mocks__/style.js"
    }
  }
}
